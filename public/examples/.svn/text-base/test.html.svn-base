<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:v="urn:schemas-microsoft-com:vml">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <title>Google Maps : MyMaps GeometryControls Example</title>
    <script type="text/javascript">
    	var host = window.location.host;
      var scriptSrc = '<script src="http://maps.google.com/maps?file=api&amp;v=2.148';
      if (host.indexOf("9082") > -1) {
        document.write(scriptSrc + '&amp;key=ABQIAAAArrEiatXGuzeUbfJWQR3AhBTbJ68HlRqlOuTJhBjF2X61zNUI4BQAywr5dsME4lVekkhpcwC54RHgnA" type="text/javascript"><\/script>');
      } else if (host.indexOf("gmaps-utility-library-dev") > -1) {
        document.write(scriptSrc + '&amp;key=ABQIAAAAjU0EJWnWPMv7oQ-jjS7dYxQGj0PqsCtxKvarsoS-iqLdqZSKfxRdmoPmGl7Y9335WLC36wIGYa6o5Q" type="text/javascript"><\/script>');
      } else {	
        document.write(scriptSrc + '&amp;key=ABQIAAAAvGgo_y3byPY_Y4OGiNqDiBRS-E0_rcxAj8SW8U57bHIePDFkcxSDwuYoDBoubxHwdc6ase6n2xQXhw" type="text/javascript"><\/script>');
    	}
    </script>
    <script src="../src/geometrycontrols.js" type="text/javascript"></script>
    <script src="../src/markercontrol.js" type="text/javascript"></script>
    <script src="../src/polygoncontrol.js" type="text/javascript"></script>
    <script src="../src/polylinecontrol.js" type="text/javascript"></script>
    <script src="scripts/map_main.js" type="text/javascript"></script>
    <style type="text/css">
    	/* to be added to a central style sheet */
      .emmc-tooltip {
    		border: 1px solid #666666;
    		background-color: #ffffff;
    		color: #444444;
    		display:none;
        font-size:13px;
        padding:1px;
    	}
      
      /* Doesn't work in ie :(
       #msim-icons * img:hover {
        border-color:#3D69B1;
      }*/
    </style>
  </head>

  <body>
    <div id="map_canvas" style="width: 1200px; height: 500px"></div>
    <input type="button" id="autoSaveToggle" value="Turn AutoSave On" onclick="javascript:mockAutoSave(this);"/>
  </body>
</html>
