	<!--
	/**
	* Basic template for initial infowindow view
	*/-->
	<script id="template_1" evalRequired="false" type="text/html" >
  	<![CDATA[
		<div style="position:relative; left:5px; top:0px; width:279px; height:224px; z-index:10;">
		<div>
			<div>
				<div class="msedit">
					<table class="iwspan">  
						<tbody>
							<tr>
								<td>
									<table class="inputField">
										<tbody>
											<tr>
												<td class="label">Title</td>
												<td>
													<input class="title" id="emmc-geom-title" type="text" maxlength="250" dir="ltr"/>
												</td>
												<td>
													<div id="msiwsi" class="icon">
														<%=geometry_style_link%>
													</div>
												</td>
											</tr>
										</tbody>
									</table>
									<table class="inputField">
										<tbody>
											<tr>
												<td class="label">Description</td>
												<td class="tabs">
													<table>
														<tbody>
															<tr>
																<td>
																	<span class="stab">Plain text</span>
																</td>
																<!-- 
																	<td class="stab"> - </td>
																	<td>
																		<span class="lk">Rich text</span>
																	</td>
																	<td class="stab"> - </td>
																	<td>
																	<span class="lk">Edit HTML</span>
																	</td>
																-->
															</tr>
														</tbody>
													</table>
												</td>
											</tr>
										</tbody>
									</table>
								</td>
							</tr>
						</tbody>
					</table>
					<div>
						<div id="rtfield" class="textField description" style="display:none;"></div>
						<textarea class="textField description" id="emmc-geom-description" dir="ltr"></textarea>
						<div style="font-size: 1%; width: 264px; height: 1px;"></div>
					</div>
					<div class="msfeatureupdater msfeatureupdaterbot" style="">
						<!-- Last Updated by
						<a href="add a variable">put some data in here</a>
						1 hours ago
						-->
					</div>
					<table>
						<tbody>
							<tr>
								<td class="navLeft">
									<span class="lk" id="emmc-geom-delete">Delete</span>
								</td>
								<td class="navRight">
									<button id="emmc-geom-cancel">Cancel</button>
								</td>
								<td class="navRight">
									<button id="emmc-geom-ok">OK</button>
								</td>
							</tr>
						</tbody>
					</table>
				</div>

		]]>
	</script>
	
